{
   "background": {
      "scripts": [ "core/background.js" ]
   },
   "content_scripts": [ {
      "js": [ "lib/jquery.min.js", "lib/fontawesome.js", "core/crawlerService.js", "core/drawService.js", "main.js" ],
      "matches": [ "*://*.safaribooksonline.com/*" ],
      "run_at": "document_idle"
   } ],
