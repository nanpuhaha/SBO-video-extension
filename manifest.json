   "key": "",
   "manifest_version": 2,
   "name": "Safari Books Online Video Downloader",
   "permissions": [ "cookies", "downloads", "*://*.safaribooksonline.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.6"
}
